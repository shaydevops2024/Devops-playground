{
  "uid": "execution-statistics",
  "title": "Execution Statistics",
  "tags": ["executions", "statistics"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Executions",
      "targets": [
        {
          "expr": "sum(devops_playground_executions_total) OR on() vector(0)",
          "refId": "A",
          "instant": true
        }
      ],
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 0 },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Successful",
      "targets": [
        {
          "expr": "sum(devops_playground_executions_successful) OR on() vector(0)",
          "refId": "A",
          "instant": true
        }
      ],
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 0 },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "green" }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Failed",
      "targets": [
        {
          "expr": "sum(devops_playground_executions_failed) OR on() vector(0)",
          "refId": "A",
          "instant": true
        }
      ],
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 0 },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "red" }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Currently Running",
      "targets": [
        {
          "expr": "devops_playground_active_executions OR on() vector(0)",
          "refId": "A",
          "instant": true
        }
      ],
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 0 },
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" }
            ]
          },
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Execution Rate",
      "targets": [
        {
          "expr": "sum(rate(devops_playground_executions_total[1m])) OR on() vector(0)",
          "refId": "A",
          "legendFormat": "Executions/sec"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Success vs Failed",
      "targets": [
        {
          "expr": "sum(devops_playground_executions_successful) OR on() vector(0)",
          "refId": "A",
          "legendFormat": "Successful"
        },
        {
          "expr": "sum(devops_playground_executions_failed) OR on() vector(0)",
          "refId": "B",
          "legendFormat": "Failed"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Executions by Playground",
      "targets": [
        {
          "expr": "sum by (playground) (devops_playground_executions_total) OR on() vector(0)",
          "refId": "A",
          "legendFormat": "{{playground}}"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 13 }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Execution Duration (95th percentile)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(devops_playground_execution_duration_seconds_bucket[1m]))) OR on() vector(0)",
          "refId": "A",
          "legendFormat": "95th percentile"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 13 },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    }
  ]
}